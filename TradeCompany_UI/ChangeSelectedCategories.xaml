<Window x:Class="TradeCompany_UI.ChangeSelectedCategories"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TradeCompany_UI"
        mc:Ignorable="d"
        Title="" FontSize="14" ResizeMode="NoResize" Width="500" Height="250">
    <Grid Margin="0,0,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="10" />
            <RowDefinition Height="80" />
        </Grid.RowDefinitions>

        <WrapPanel Grid.Row="2" HorizontalAlignment="Center" Margin="0,0,0,47" Width="324">
            <Button IsDefault="True" Click="Accept_Click" MinWidth="60" Height="32" Margin="9,0,0,0" Width="101" Content="Отмена"/>
            <Button x:Name="DeleteButton" IsCancel="True" MinWidth="60" Height="32" Margin="15,0,0,0" Width="189" Content="Удалить выбранные" Click="DeleteButton_Click"/>
        </WrapPanel>

        <StackPanel x:Name="StackPanel" Height="Auto" Margin="20,20,38,0" Grid.Row="0"/>

    </Grid>
</Window>
